<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!--disabled-->
    <item android:state_enabled="false" android:drawable="@drawable/button_save_enabled" />

    <!--pressed-->
    <item android:state_pressed="true" android:state_enabled="true" android:drawable="@drawable/button_save_pressed" />

    <!--focused-->
    <item android:state_focused="true" android:state_enabled="true" android:drawable="@drawable/button_save_enabled" />

    <!--enabled-->
    <item android:state_enabled="true" android:drawable="@drawable/button_save_enabled" />

</selector>